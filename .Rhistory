colon_csv <- "http://dl.dropboxusercontent.com/s/vfq8k5nv4h2g2t7/colon_data.csv?token_hash=AAH-bvR0QIFqmGkJIOerHjgnTXqlAHqx65VNF0rmATNd0g&dl=1"
e <- read.csv(colon_csv, stringsAsFactors=FALSE)
# # handle NA's
#
# # all count variables can be a given a "0" if NA
na2zero <- function(x) {
x[is.na(x)] <- 0
return(x)
}
counts <- grep("^n_", names(e))
e[,counts] <- apply(e[,counts], 2, na2zero)
# # edge cases
e$event_path[is.na(e$event_path)] <- "exit"
e$all_chapters[is.na(e$all_chapters)] <- 0
e$max_chapter <- e$max_chapter + 1
e$max_chapter[is.na(e$max_chapter)] <- 0
e$reached_comments[is.na(e$reached_comments)] <- 0
e$ismobile[is.na(e$ismobile)] <- 0
e$iscomputer[is.na(e$iscomputer)] <- 0
e$istablet[is.na(e$istablet)] <- 0
e$time_for_events[is.na(e$time_for_events)] <- 0
e$dateest <- ymd_hms(e$dateest)
e$hour <- hour(e$dateest)
e$day <- day(e$dateest)
e$min <- minute(e$dateest)
e$bucket_hour <- paste(e$day, e$hour, sep=":")
